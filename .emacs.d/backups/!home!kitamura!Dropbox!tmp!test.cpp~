#include <iostream>
#include <limits>
using namespace std;


void g(char c, signed char sc, unsigned char uc)
{
  c = 10;
  cout << +c << c <<  "\n";
  c = sc;
  cout << +c << c << "\n";
  c = uc;
  cout << +c << c << "\n";
  sc = uc;
  cout << +sc << sc << "\n";
  sc  = c;
  cout << +sc << sc << "\n";
  uc = c;
  cout << +uc << uc << "\n";  
	  
}

int main() {
  char p[] = "asdfgh";
  for (char *pp = p; *pp!=0; ++pp)
	cout << *pp << "\n";
  return 0;
}
